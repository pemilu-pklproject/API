      "/si-pemilu/api/v1/login/admin": {
        "post": {
          "tags": ["Login"],
          "summary": "Admin login",
          "description": "Mengembalikan access_token dan refresh_token",
          "requestBody": {
            "description": "Data yang dikirimkan",
            "content": {
              "application/json": {
                "example": {
                  "email": "email",
                  "passowrd": "password"
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Sukses",
              "content": {
                "application/json": {
                  "example": {
                    "status": true,
                    "id": "id",
                    "nama": "nama",
                    "access_token": "access_token",
                    "refresh_token": "refresh_token"
                  }
                }
              }
            },
            "401": {
              "description": "Unauthorized",
              "content": {
                "application/json": {
                  "example": {
                    "status": false,
                    "msg": "email dan Password salah"
                  }
                }
              }
            },
            "500": {
              "description": "Server Error"
            }
          }
        }
      },
